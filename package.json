{
  "name": "nest-boilerplate",
  "version": "1.0.0",
  "description": "Nest Js api boilerplate example",
  "author": "Mikael Hadler",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development AWS_REGION=us-east-1 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "tslint --config tslintrc.json --project tslintrc.json",
    "lint:report": "eslint -f json -o coverage/report.json src/",
    "test": "TZ=UTC cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "test:watch": "TZ=UTC cross-env NODE_ICU_DATA=node_modules/full-icu jest --watch",
    "test:coverage": "TZ=UTC cross-env NODE_ICU_DATA=node_modules/full-icu jest --coverage",
    "test:debug": "TZ=UTC cross-env NODE_ICU_DATA=node_modules/full-icu node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "TZ=UTC cross-env NODE_ICU_DATA=node_modules/full-icu jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/terminus": "^7.0.1",
    "aws-sdk": "^2.905.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "currency-formatter": "^1.5.6",
    "currency-symbol-map": "^4.0.4",
    "dayjs": "^1.8.36",
    "form-data": "^4.0.0",
    "full-icu": "^1.3.1",
    "google-auth-library": "^6.1.3",
    "http-status": "^1.4.2",
    "husky": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "jwks-ecdsa": "^1.0.3",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "nanoid": "^3.1.12",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "tslint": "^6.1.3",
    "tslint-plugin-prettier": "^2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.10.4",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/testing": "^7.0.0",
    "@types/currency-formatter": "^1.3.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.160",
    "@types/lodash.repeat": "^4.1.6",
    "@types/multer": "^1.4.5",
    "@types/node": "^13.9.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.25",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "dayjs": "^1.8.36",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "26.5.6",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
